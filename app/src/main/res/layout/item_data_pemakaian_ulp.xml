<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="25dp"
    android:layout_marginEnd="25dp"
    android:layout_marginTop="15dp"
    app:cardCornerRadius="5dp"
    android:background="#F7F8FA">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <View
            android:id="@+id/view"
            android:layout_width="10dp"
            android:layout_height="0dp"
            android:background="#26ACE2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lbl_no_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="No. Reservasi"
            android:textSize="10sp"
            app:layout_constraintEnd_toStartOf="@id/lbl_no_do"
            app:layout_constraintStart_toEndOf="@id/view"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/lbl_no_do"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:src="@drawable/img_listrik"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="22dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/lbl_no_reservasi"
            app:layout_constraintTop_toTopOf="@id/lbl_no_reservasi"
            app:layout_constraintBottom_toBottomOf="@id/lbl_no_reservasi"/>

        <TextView
            android:id="@+id/txt_no_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="10011"
            android:textColor="#3F3D56"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/lbl_no_reservasi"
            app:layout_constraintStart_toStartOf="@+id/lbl_no_reservasi"
            app:layout_constraintTop_toBottomOf="@id/lbl_no_reservasi" />

        <TextView
            android:id="@+id/txt_no_do"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/montserrat_semibold"
            android:text="10011"
            android:textColor="#3F3D56"
            android:textSize="12sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="@+id/lbl_no_do"
            app:layout_constraintStart_toStartOf="@+id/lbl_no_do"
            app:layout_constraintTop_toBottomOf="@id/lbl_no_do" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_reservasi"
            app:layout_constraintTop_toBottomOf="@+id/txt_tgl_reservasi" />

        <TextView
            android:id="@+id/lbl_jenis_pekerjaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Jenis Pekerjaan"
            android:textSize="10sp"
            android:textColor="#3F3D56"
            android:fontFamily="@font/montserrat_reguler"
            android:layout_marginStart="5dp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_reservasi"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="@+id/imageView2" />

        <TextView
            android:id="@+id/txt_jenis_pekerjaan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="1110011"
            android:textSize="10sp"
            app:layout_constraintBottom_toTopOf="@id/lbl_sumber_reservasi"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_reservasi"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/lbl_jenis_pekerjaan" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_reservasi"
            app:layout_constraintTop_toBottomOf="@+id/txt_jenis_pekerjaan" />

        <TextView
            android:id="@+id/lbl_sumber_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Sumber Reservasi"
            android:textSize="10sp"
            android:textColor="#3F3D56"
            android:fontFamily="@font/montserrat_reguler"
            android:layout_marginStart="5dp"
            app:layout_constraintEnd_toEndOf="@+id/txt_jenis_pekerjaan"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            app:layout_constraintTop_toTopOf="@+id/imageView6" />

        <TextView
            android:id="@+id/txt_sumber_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="1110011"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/txt_jenis_pekerjaan"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/lbl_sumber_reservasi" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_do"
            app:layout_constraintTop_toBottomOf="@+id/txt_status_reservasi" />

        <TextView
            android:id="@+id/lbl_status_kirim_sap_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_reguler"
            android:layout_marginStart="5dp"
            android:text="Status Kirim SAP Reservasi"
            android:textSize="10sp"
            android:textColor="#3F3D56"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_do"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toTopOf="@+id/imageView3" />

        <TextView
            android:id="@+id/txt_status_kirim_sap_reservasi"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="xxxxx"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_jenis_pekerjaan"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_do"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/lbl_status_kirim_sap_reservasi" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_reservasi"
            app:layout_constraintTop_toBottomOf="@+id/txt_no_reservasi" />

        <TextView
            android:id="@+id/lbl_tgl_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Tanggal Reservasi"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_reservasi"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toTopOf="@+id/imageView4" />

        <TextView
            android:id="@+id/txt_tgl_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="xxxxxx"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/lbl_tgl_reservasi"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toBottomOf="@+id/lbl_tgl_reservasi" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_number"
            app:layout_constraintStart_toStartOf="@+id/txt_no_do"
            app:layout_constraintTop_toBottomOf="@+id/txt_no_do" />

        <TextView
            android:id="@+id/status_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Status Reservasi"
            android:textColor="#3F3D56"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/txt_no_do"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toTopOf="@+id/imageView5" />

        <TextView
            android:id="@+id/txt_status_reservasi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="xxxxxx"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="@+id/status_reservasi"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/status_reservasi" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>