        <TextView
            android:id="@+id/lbl_input_pemakaian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Input Pemakaian"
            android:textColor="#3F3D56"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/cv_no_reservasi"
            app:layout_constraintTop_toBottomOf="@+id/cv_no_reservasi" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_pemakaian"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:background="@drawable/bg_btn1"
            android:fontFamily="@font/montserrat_medium"
            android:text="Input Pemakaian"
            android:textAllCaps="false"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/lbl_input_pemakaian"
            app:layout_constraintEnd_toEndOf="@+id/cv_no_reservasi"
            app:layout_constraintTop_toTopOf="@+id/lbl_input_pemakaian" />

        <TextView
            android:id="@+id/txt_nomorMaterial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="No. Material"
            android:textColor="#3F3D56"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_pemakaian" />

            <EditText
                android:id="@+id/src_nomorMaterial"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginStart="23dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="25dp"
                android:background="@drawable/srchview"
                android:drawableEnd="@drawable/ic_search"
                android:fontFamily="@font/montserrat_reguler"
                android:hint="Search here"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_nomorMaterial" />

            <View
                android:id="@+id/view3"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="15dp"
                android:background="#B9B9B9"
                app:layout_constraintEnd_toEndOf="@id/src_nomorMaterial"
                app:layout_constraintStart_toStartOf="@id/src_nomorMaterial"
                app:layout_constraintTop_toBottomOf="@id/src_nomorMaterial" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_pemakaian_ulp"
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginBottom="20dp"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/txt_total_data"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view3"
            tools:listitem="@layout/item_data_detail_pemakaian_ulp" />

        <TextView
            android:id="@+id/txt_total_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:fontFamily="@font/montserrat_medium"
            android:text="total 0 data"
            android:textColor="#96979B"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@id/btn_simpan"
            app:layout_constraintStart_toStartOf="@+id/src_nomorMaterial" />

            <ProgressBar
                android:id="@+id/progress_bar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view3" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_simpan"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/br_btn"
            android:elevation="8dp"
            android:fontFamily="@font/montserrat_reguler"
            android:text="Selesai"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>